import java.util.*;
 class Arithmetic {
	void sum(int a,int b) {
		System.out.println(a+b);
	}
	void difference(int a,int b){
		System.out.println(a-b);
	}
}
class Main{
	public static void main(String args[]){
		Scanner sc = new Scanner(System.in);
		int a=sc.nextInt();
		int b=sc.nextInt();
		Arithmetic obj = new Arithmetic();
		if(a>b){
		    obj.difference(a,b);
		}
		else {
		    obj.sum(a,b);
		}
	}
}
